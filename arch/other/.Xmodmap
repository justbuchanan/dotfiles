! TODO: delete this file once we've fully moved from X11 to wayland.
! The config here is obsoleted by /etc/keyd/default.conf.


!remap caps lock to Super key
keycode 66 = Super_L
clear Lock
keycode 0x42 = Super_L

! multimedia keys for WASD v2
keycode 118 = XF86AudioPlay
keycode 119 = XF86AudioPrev
keycode 115 = XF86AudioNext
keycode 127 = XF86AudioMute
keycode 110 = XF86AudioStop
keycode 112 = XF86AudioRaiseVolume
keycode 117 = XF86AudioLowerVolume
